{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"#server/*": [
				"src/*"
			]
		},
		"target": "esnext",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": false,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": false,
		"types": [
			"jest"
		]
	},
}